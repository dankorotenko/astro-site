---
import Nav from "../components/Nav";
const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta
      name="description"
      content="Digital Artist Services, accessible for everyone"
    />
    <meta title={title} />
    <title>{title}</title>
  </head>
  <body>
    <main>
      <Nav client:load/>
      <slot />
    </main>
  </body>
</html>
